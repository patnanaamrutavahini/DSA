/*
1
23
456
78910
*/
#include <iostream>
using namespace std;
void fun(int n){
    int num=1;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=i;j++){
            cout<<num;
            num=num+1;
        }
        cout<<endl;
        
    }
}
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int n;
        cin>>n;
        fun(n);
    }
    return 0;
}
