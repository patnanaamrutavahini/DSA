#include <iostream>
using namespace std;
void fun(int n){
    int star=1;
    for(int i=0;i<n;i++){              //1
        if(i%2==0) star=1;             //01
        else star=0;                   //101
        for(int j=0;j<=i;j++){         //0101
            cout<<star;
            star=1-star;
        }
        cout<<endl;
    }
}
int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        int n;
        cin>>n;
        fun(n);
    }
    return 0;
}
